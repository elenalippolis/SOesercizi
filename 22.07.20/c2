//va bene ma non va fatto con db, il prof l'ha implementato con una struttura dati esterna static (L: list of T)

void tsend(msg_t msg, pid_t dest){
  asend(msg, dest);
}

msg_t trecv(){
  bool 2msg = false;
  while(2msg==false){
    msg = arecv(ANY);
    if(db.found(msg){
      db.remove(msg);
      2msg=true;
    } else {
      db.add(msg);
    }
  }
  return(msg);
}
